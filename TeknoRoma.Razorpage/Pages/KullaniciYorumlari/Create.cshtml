@page
@model TeknoRoma.Razorpage.Pages.KullaniciYorumlari.CreateModel
@{
    ViewData["Title"] = "KullaniciId Ekle";
}
<h1>Yeni Kullanici Ekle</h1>
<div class="card">
    <div class="card-header">Kullanici Bilgileri</div>
    <div class="card-body">
        <form method="post">
            <div class="form-group">
                <label asp-for="@Model.InsertModel.KullaniciId"></label>
                <input asp-for="InsertModel.KullaniciId" class="form-control" />
                <span asp-validation-for="InsertModel.KullaniciId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="InsertModel.StokId"></label>
                <input asp-for="InsertModel.StokId" class="form-control" />
                <span asp-validation-for="InsertModel.StokId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="@Model.InsertModel.YorumMetni"></label>
                <input asp-for="InsertModel.YorumMetni" class="form-control" />
                <span asp-validation-for="InsertModel.YorumMetni" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="@Model.InsertModel.YorumTarihi"></label>
                <input asp-for="InsertModel.YorumTarihi" class="form-control" />
                <span asp-validation-for="InsertModel.YorumTarihi" class="text-danger"></span>
            </div>
            <button type="submit" class="btn btn-primary">Kaydet</button>
        </form>
    </div>
</div>
<partial name="_ValidationScriptsPartial" />
